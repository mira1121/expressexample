{
  "ignore": ["node_nodules/**/*"],
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "esmodules": true
        }
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-transform-modules-commonjs"
  ],
  "sourceMaps": true,
  "retainLines": true,
  "comments": false
}
